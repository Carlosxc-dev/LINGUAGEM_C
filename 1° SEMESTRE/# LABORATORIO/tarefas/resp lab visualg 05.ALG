algoritmo "PrimeiroProg"
var
   gra, rad : real
inicio
   escreval("--- calculadora de radianos ---")
   escreval()
   escreva("digite ângulo em graus: ")
   leia(gra)
   escreval()
   rad <- gra * pi / 180
   escreval("Voce digitou ", gra, " graus")
   escreval("Em radianos fica ", rad)
fimalgoritmo